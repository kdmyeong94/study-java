package innerTest;

public class Outer {
	
	int ov = 10;
	
	public void of() {

		
		class InnerLocal{ // 지역 클래스
			int liv = 40;
			
			public void hi() {
				System.out.println("InnerLocal클래스 속의 메서드");
			}
			
		}
		
		InnerLocal il = new InnerLocal();
		System.out.println(il.hi());

		System.out.println("일반 메서드");
		
	}
	
}
