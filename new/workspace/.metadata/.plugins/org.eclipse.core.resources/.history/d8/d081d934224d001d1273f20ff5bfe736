package day10;

import java.util.Scanner;

public class MethodTest {

	public static void main(String[] args) {
		// 사용자에게 나이를 입력받아서 미성년자인지 아닌지 판단하는 메서드
		Scanner sc = new Scanner(System.in);
		int age = sc.nextInt();
		
		if ( age > 19) {
			System.out.println("성인입니다.");
		} else {
			System.out.println("미성년자입니다.");
		}
		
		
		
	}
}
