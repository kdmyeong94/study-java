package string;

public class Main02 {

	public static void main(String[] args) {
		
//		1. 이메일 주소에서 아이디와 도메인을 구별하기
//		단, splite 사용 금지
		
		String email = "student@java.com";
		
//		결과 : 아이디 -> student
//		결과 : 도메인 -> java.com
		
		String a = email.substring(0 , 7);
		String b = email.substring(8);
		System.out.println("아이디 : " + a);
		System.out.println("도메인 : " + b);
	}
}
